{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport SearchSelector from \"./SearchSelector/SearchSelector\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"Search\",\n  data() {\n    return {\n      searchParams: {\n        // 一级分类id\n        category1Id: \"\",\n        // 二级分类id\n        category2Id: \"\",\n        // 三级分类id\n        category3Id: \"\",\n        // 分类名字\n        categoryName: \"\",\n        // 关键字\n        keyword: \"\",\n        // 排序(应该给一个初始值：降序排序|综合)\n        order: \"1:desc\",\n        // 分页器\n        pageNo: 1,\n        pageSize: 10,\n        // 平台售卖属性操作的带参数\n        props: [],\n        // 品牌\n        trademark: \"\"\n      }\n    };\n  },\n  components: {\n    SearchSelector\n  },\n  //组件挂载之前执行一次\n  beforeMount() {\n    Object.assign(this.searchParams, this.$route.query, this.$route.params);\n  },\n  mounted() {\n    this.getData();\n  },\n  computed: {\n    ...mapGetters([\"goodsList\"]),\n    // 判断是哪种属性\n    isOne() {\n      return searchParams.order.indexOf('1') != -1;\n    },\n    isTwo() {\n      return searchParams.order.indexOf('2') != -1;\n    }\n  },\n  methods: {\n    // 向服务器发起请求，获取search模块下的数据\n    getData() {\n      this.$store.dispatch(\"getSearchList\", this.searchParams);\n    },\n    // 删除分类的名字\n    removeCategoryName() {\n      this.searchParams.categoryName = undefined;\n      this.searchParams.category1Id = undefined;\n      this.searchParams.category2Id = undefined;\n      this.searchParams.category3Id = undefined;\n      // 地址栏也需要改，自己跳自己\n      if (this.$route.params) {\n        this.$router.push({\n          name: 'search',\n          params: this.$route.params\n        });\n      } else {\n        this.$router.push(this.$route.path);\n      }\n    },\n    // 删除关键字\n    removeKeyword() {\n      // 给服务器携带参数searchParams的keyword置空\n      this.searchParams.keyword = undefined;\n      this.getData();\n      // 通知兄弟组件Header组件干掉搜索框的内容\n      this.$bus.$emit(\"clear\");\n      // 清除地址栏的query参数\n      if (this.$route.query) {\n        this.$router.push({\n          name: 'search',\n          query: this.$route.query\n        });\n      }\n    },\n    // 自定义事件回调\n    trademarkInfo(trademark) {\n      // 整理参数（ID:品牌名称）\n      this.searchParams.trademark = `${trademark.tmId}:${trademark.tmName}`;\n      this.getData();\n    },\n    // 删除品牌的面包屑 \n    removeTradeMark() {\n      this.searchParams.trademark = undefined;\n      if (this.$route.params) {\n        this.$router.push({\n          name: 'search',\n          params: this.$route.params\n        });\n      }\n    },\n    // 收集平台属性的地方\n    attrInfo(attr, attrValue) {\n      // 整理参数\n      let props = `${attr.attrId}:${attrValue}:${attr.attrName}`;\n      // 数组去重\n      if (this.searchParams.props.indexOf(props) == -1) {\n        this.searchParams.props.push(props);\n        // 再次发起请求\n        this.getData();\n      }\n    },\n    // 移除售卖属性\n    removeAttr(index) {\n      this.searchParams.props.splice(index, 1);\n      this.getData();\n    }\n  },\n  watch: {\n    //  监听数据：监听组件实例身上的属性的属性值是否发生变化\n    $route(newValue, oldValue) {\n      // 再次发起请求之前整理数据\n      Object.assign(this.searchParams, this.$route.query, this.$route.params);\n      // 再次发起请求\n      this.getData();\n      // 每次请求完毕，应该吧1、2、3级的分类id滞空，让他接收下一次相应的id\n      this.searchParams.category1Id = undefined;\n      this.searchParams.category2Id = undefined;\n      this.searchParams.category3Id = undefined;\n    }\n  }\n};","map":{"version":3,"mappings":";AAqHA;AACA;AACA;EACAA;EACAC;IACA;MACAC;QACA;QACAC;QACA;QACAC;QACA;QACAC;QACA;QACAC;QACA;QACAC;QACA;QACAC;QACA;QACAC;QACAC;QACA;QACAC;QACA;QACAC;MACA;IACA;EACA;EACAC;IACAC;EACA;EACA;EACAC;IACAC;EACA;EACAC;IACA;EACA;EACAC;IACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACA;IACAC;MACA;IACA;IACA;IACAC;MACA;MACA;MACA;MACA;MACA;MACA;QACA;UAAAvB;UAAAwB;QAAA;MACA,OACA;QACA;MACA;IACA;IACA;IACAC;MACA;MACA;MACA;MACA;MACA;MACA;MACA;QACA;UAAAzB;UAAA0B;QAAA;MACA;IACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACA;IA